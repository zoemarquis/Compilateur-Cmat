int main() {
  matrix a[3][2] = {{1, 2}, {3, 4}, {5.5, 6.6}};
  printmat(a);
  printf("\n");

  a++;
  printmat(a);
  printf("\n");

  ++a;
  printmat(a);
  printf("\n");

  a--;
  printmat(a);
  printf("\n");

  --a;
  printmat(a);
  printf("\n");

  return 0;
}
